{
  "manifest_version": 2,
  "version": "0.1.3",
  "name": "GitHub Project Estimates",
  "description": "Adds estimated completion times to the Projects view of GitHub issues",
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png",
    "180": "assets/icon180.png",
    "192": "assets/icon192.png",
    "512": "assets/icon512.png"
  },
  "content_scripts": [
    {
      "css": [
        "statics/styles.css"
      ],
      "js": [
        "src/index.js"
      ],
      "matches": [
        "https://github.com/*"
      ]
    }
  ]
}